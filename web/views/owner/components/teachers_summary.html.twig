<div class="panel panel-info">
    <h2 class="panel-heading" >Teachers </h2>
    {% if teachers is not empty %}
    <div class="panel-body">
        <h4 class="stats-numbers">Total: {{ teachers|length  | e }}</h4>
        <ul>
            {% for teacher in teachers %}
            <li><a href="teacher/{{teacher.getId | e }}">{{teacher.getName | e }}</a></li>
            {% endfor %}
        </ul>
        {% endif %}
    </div>
</div>
