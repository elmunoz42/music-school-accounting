<h3>Student Info:</h3>
<hr>
{% if students_teachers is not empty %}
    <h4 class="panel-heading">
    <table class="table table-hover">
        <thead>
            <th>Name</th>
        </thead>
        <tbody>
            {% for student in students_teachers %}
            <tr>
                <td><a href="/owner_student/{{ student.getId  | e }}">{{ student.getName  | e }}</a></td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
{% endif %}
